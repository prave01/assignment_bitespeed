{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames"
  ],
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": [
        "*.tsx",
        "*.jsx",
        "*.html"
      ],
      "options": {
        "printWidth": 80
      }
    }
  ]
}
